{
  "version": "8.0.0-next.0",
  "npmClient": "yarn",
  "command": {
    "publish": {
      "allowBranch": "**/*",
      "ignoreChanges": [
        "**/test/**",
        "**/*.md"
      ],
      "forcePublish": true,
      "npmClient": "yarn",
      "message": "chore(release): publish %s"
    }
  },
  "$schema": "node_modules/lerna/schemas/lerna-schema.json"
}