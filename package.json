{
  "name": "microdi-js",
  "version": "1.0.1",
  "description": "A tiny DI container",
  "keywords": [
    "di",
    "dependency injection",
    "inversion of control",
    "ioc",
    "container",
    "unity",
    "object builder"
  ],
  "scripts": {
    "start": "npm run test",
    "test": "jest --watchAll --no-cache --config jest.config",
    "test:ci": "jest --teamcity --config jest.config --verbose",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:prod": "webpack -p --config webpack.prod.config.js"
  },
  "main": "dist/microdi.js",
  "author": "Dev Shop Limited",
  "license": "Apache-2.0",
  "repository": "https://github.com/KeithWoods/microdi-js.git",
  "typings": "./microdi-js.d.ts",
  "devDependencies": {
    "babel-jest": "^18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "eslint": "3.12.2",
    "eslint-loader": "^1.6.1",
    "jasmine-core": "2.5.2",
    "jest": "18.1.0",
    "jest-teamcity-reporter": "0.3.0",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  }
}
